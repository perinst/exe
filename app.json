{
  "expo": {
    "name": "palette",
    "slug": "palette",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-palette.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pernist.palette",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon-palette.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.pernist.palette"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "edccffb0-237c-4984-a216-ac672e5bf7bb"
      }
    },
    "owner": "pernist"
  }
}
